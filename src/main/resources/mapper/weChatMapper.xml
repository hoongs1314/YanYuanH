<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.Beam.mapper.weChatMapper">

    <select id="findData" resultType="java.lang.String">
        select tableName
        from wechattest
        where id = #{data}
    </select>

    <!--添加用户信息-->
    <insert id="insertCustomer" parameterType="java.util.Map">
        insert into customerinfomation (user_name,user_dormitory,user_phone,user_sex)
        values (#{userName},#{userRoom},#{userPhone},#{userSex})
    </insert>

    <!--查询商家外卖-->
    <select id="findMenu" resultType="java.util.Map">
        select *
        from DaZuiBuDaiMo
    </select>
</mapper>